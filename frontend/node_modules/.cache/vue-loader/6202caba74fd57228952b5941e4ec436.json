iv v-else>\n        <p>{{message}}</p>\n        <button @click=\"anotherSearchTour\">Search again</button>\n    </div>\n</template>\n<script>\n    import http from \"../http-common\"\n    export default{\n        data(){\n            return{\n               tour : {\n                     book_id: 0\n               },\n               submitted: false,\n               found: false,\n               errFound: false,\n               foundTours : [],\n               message: \"\"\n            }\n        },\n        methods : {\n            searchTheTour(){\n                http\n                    .get(\"tours/byBookId/\"+this.tour.book_id)\n                    .then((response) => {\n                         if(response.data.status==\"success\"){\n                            this.found = true\n                            this.foundTours = response.data.tours\n                         }\n                         else if(response.data.status==\"not found\"){\n                            this.errFound = true\n                            this.message = response.data.message\n                         }\n\n                    this.submitted = true\n                    })\n            },\n            anotherSearchTour(){\n                this.submitted = false,\n                this.found = false,\n                this.errFound = false,\n                this.tour = {}\n            }\n        }\n    }\n</script>"]}]}