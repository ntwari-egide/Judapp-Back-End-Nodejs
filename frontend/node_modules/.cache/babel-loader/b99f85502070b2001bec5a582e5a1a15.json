,8BAAA,KAAA,CAAA,aAAA,GAAA,IAAA;AACA,6BAFA,MAGA;AACA,8BAAA,KAAA,CAAA,mBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,8BAAA,KAAA,CAAA,QAAA,CAAA,OAAA;;AACA,8BAAA,KAAA,CAAA,QAAA,CAAA,KAAA;;AACA,8BAAA,KAAA,CAAA,QAAA,CAAA,GAAA,CAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA,KAAA;;AACA,8BAAA,KAAA,CAAA,MAAA,CAAA;AACA,gCAAA,OAAA,EAAA;AACA,mDAAA,YAAA,QAAA,CAAA,IAAA,CAAA;AADA;AADA,+BAAA;;AAKA,8BAAA,KAAA,CAAA,QAAA,CAAA,GAAA,CAAA,QAAA,EAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AAEA,8BAAA,IAAA,CAAA,GAAA,CAAA,0DAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,gCAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,gCAAA,KAAA,CAAA,QAAA,CAAA,GAAA,CAAA,YAAA,EAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,+BAJA;AAKA;;AAtBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAFA;;AAAA;AAAA;AAAA;AAAA;AA4BA,qBAAA,SAAA,GAAA,IAAA;;AAjCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OADA;AAoCA,IAAA,aAAA,EAAA,uBAAA,QAAA,EAAA,WAAA,EAAA,SAAA,EAAA;AACA,WAAA,QAAA,CAAA,GAAA,CAAA,UAAA,EAAA,QAAA;AACA,WAAA,QAAA,CAAA,GAAA,CAAA,aAAA,EAAA,WAAA;AACA,WAAA,QAAA,CAAA,GAAA,CAAA,WAAA,EAAA,SAAA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,YAAA;AACA,KAzCA;AA0CA,IAAA,UAAA;AAAA,gFAAA;AAAA;AAAA;AAAA;AAAA;AACA,qBAAA,SAAA,GAAA,KAAA;AACA,qBAAA,aAAA,GAAA,KAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA1CA;AAlBA,CAAA","sourcesContent":["<template>\n    <div class=\"login-page\">\n        <router-link class=\"go-on-other-page\" :to=\"{name: 'attorney-login'}\"> Login Attorney &rarr;</router-link>\n        <div class=\"container loginContainer row align-items-center\">            \n            <div class=\"login-page-title\">\n                <h2>LOGIN CLIENT</h2>\n            </div>\n            <div class=\"form-container\" v-if=\"!submitted\">\n                <hr class=\"top-line\">\n                <input type=\"text\" placeholder=\"Phone number\" v-model=\"cleint.phoneNumber\"> <br><br>\n                <input type=\"password\" placeholder=\"Password\" v-model=\"cleint.password\"> <br><br>\n                <button @click=\"signupNow\">Login Now</button><br>\n                  <router-link id=\"loginButton\" class=\"Idont-have\" :to=\"{name: 'signupClient'}\"> I don't have account ,<span>Create account</span></router-link>\n                <hr class=\"bottom-line\">\n            </div>\n            <div  v-if=\"submitted\">\n                <div class=\"continueToDashbord\" v-if=\"notAuthorized\">\n                    <p class=\"text-danger\">Please , your login credentials are not correct</p>\n                    <button @click=\"loginAgain\">login again </button>\n                </div>\n                <div class=\"continueToDashbord\" v-else>\n                    <p class=\"text-success\">Authenticated now</p>\n                    <li v-for=\"(works, index) in workspace\" :key=\"index\" style=\"display: none\">\n                       {{workspaceId = works._id}}\n                       {{ cabinetId = works.cabinetId}}\n                    </li> \n                     <li v-for=\"(client, index) in authenticatedClient\" :key=\"index\" style=\"display: none\">\n                       {{ clientId = client._id}}                       \n                    </li>\n                    <button @click=\"gotoDashboard(clientId,workspaceId,cabinetId)\">Continue To Dashboard </button>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\n    import http from \"../http-common\"\n    import axios from \"axios\";\n    import router from \"../router\"\n    export default({\n       name: \"signupClient\",\n       data(){\n           return{\n               cleint:{                \n                phoneNumber:\"\",                \n                password:\"\"\n               },\n               authenticatedClient : this.$session.get(\"client\"),\n               clientId: \"\",\n               workspaceId: \"\",\n               cabinetId: \"\",\n               workspace : this.$session.get(\"workspaces\"),\n               submitted: false,\n               notAuthorized: false,\n               message: \"\"\n           }\n       },\n       methods:{\n            signupNow : async function() {\n                var data = {                    \n                    phoneNumber : this.cleint.phoneNumber,\n                    password : this.cleint.password\n                }\n                 http\n                .post(\"clients/login\",data)\n                .then( async (response)=>{\n                    this.message = await response\n                      if(response.data.results == '0'){\n                          this.notAuthorized = true\n                      }\n                      else{\n                           this.authenticatedClient = response.data.user\n                           this.$session.destroy()\n                           this.$session.start()\n                           this.$session.set('jwt',response.data.token)\n                           axios.create({\n                            headers: {\n                                  'Authorization': 'Bearer '+ response.data.token             \n                               }\n                            });  \n                            this.$session.set('client',response.data.user) \n\n                            http.get(\"/workspaces/workspaceByClientId/5eb5f68fbc4f9fc89c5f1c88\")\n                                .then( (response) => {\n                                    this.workspace = response.data.data\n                                    this.$session.set(\"workspaces\",response.data.data)                                    \n                                })   \n                      }\n                } )\n\n\n                this.submitted = true\n            },\n            gotoDashboard : function(clientId,workspaceId,cabinetId){\n                this.$session.set(\"clientId\",clientId);  \n                this.$session.set(\"workspaceId\",workspaceId);\n                this.$session.set(\"cabinetId\",cabinetId);\n                router.push(\"/dashboard\")\n            },\n            loginAgain: async function(){\n                this.submitted = false\n                this.notAuthorized = false\n            }           \n       }\n    })\n</script>"],"sourceRoot":"src/components"}]}