:["<template>\n    <div v-if=\"!errFound\">\n        <div v-if=\"!submitted\">\n            <input @keyup.enter=\"searchTheTour()\" type=\"search\" v-model=\"tour.book_id\" placeholder=\"Search by Book Id\"><br><br>\n            <strong>Press enter in order to search</strong><br><br>\n        </div>\n        <div v-else>\n            <div v-if=\"found\">\n                <router-link :to=\"{\n                            name: 'tours-details',\n                            params: {tour: foundTours,id: foundTours._id }\n                            }\">\n                    {{foundTours.firstName}} {{foundTours.secondName}}<br><br>\n                </router-link>\n                <button @click=\"anotherSearchTour\">Search Another Tour</button>\n            </div>\n        </div>\n    </div>\n    <div v-else>\n        <p>{{message}}</p>\n        <button @click=\"anotherSearchTour\">Search again</button>\n    </div>\n</template>\n<script>\n    import http from \"../http-common\"\n    export default{\n        data(){\n            return{\n               tour : {\n                     book_id: 0\n               },\n               submitted: false,\n               found: false,\n               errFound: false,\n               foundTours : [],\n               message: \"\"\n            }\n        },\n        methods : {\n            searchTheTour(){\n                http\n                    .get(\"tours/byBookId/\"+this.tour.book_id)\n                    .then((response) => {\n                         if(response.data.status==\"success\"){\n                            this.found = true\n                            this.foundTours = response.data.tours\n                         }\n                         else if(response.data.status==\"not found\"){\n                            this.errFound = true\n                            this.message = response.data.message\n                         }\n\n                    this.submitted = true\n                    })\n            },\n            anotherSearchTour(){\n                this.submitted = false,\n                this.found = false,\n                this.errFound = false,\n                this.tour = {}\n            }\n        }\n    }\n</script>"],"sourceRoot":"src/components"}]}