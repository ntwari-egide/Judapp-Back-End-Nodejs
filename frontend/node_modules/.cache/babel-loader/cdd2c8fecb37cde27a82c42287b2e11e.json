CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAHA;AAKA,KAbA;AAcA,IAAA,mBAAA,EAAA,6BAAA,UAAA,EAAA,IAAA,EAAA;AAAA;;AACA,WAAA,WAAA,GAAA,UAAA;AACA,WAAA,WAAA,GAAA,IAAA,CAFA,CAGA;;AACA,MAAA,IAAA,CAAA,GAAA,CAAA,sBAAA,UAAA,GAAA,GAAA,GAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,IAAA;AACA,SAHA,MAIA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA;AACA,OAVA;AAWA,KA7BA;AA8BA,IAAA,aAAA,EAAA,uBAAA,IAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA;AACA,UAAA,OAAA,EAAA,KADA;AAEA,UAAA,SAAA,EAAA,IAFA;AAGA,UAAA,WAAA,EAAA,KAAA,WAHA;AAIA,UAAA,cAAA,EAAA,KAAA;AAJA,SAAA;AAOA,QAAA,IAAA,CAAA,IAAA,CAAA,UAAA,EAAA,IAAA;AACA,aAAA,GAAA,GAAA,EAAA;AAEA,aAAA,mBAAA,CAAA,KAAA,WAAA,EAAA,IAAA;AACA;AACA,KA5CA;AA6CA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,qBAAA;AACA,WAAA,gBAAA;AACA;AAhDA,GAnBA;AAqEA,EAAA,OArEA,qBAqEA;AACA,SAAA,WAAA;AACA;AAvEA,CAAA","sourcesContent":["<template>\n    <div>     \n         <li v-for=\"(attorney, index) in this.$session.get('attorney')\" :key=\"index\"> \n\n<!--                            DASHBOARD STARTS HERE -->\n\n             <div class=\"main-container\">\n                <div class=\"action-pane left-pane\">\n                    <div class=\"profile\">\n                        <ul>\n                            <li>{{attorney.fullName}}</li>\n                        </ul>\n                    </div>\n                    <div class=\"location cabinet-credentials\">\n                        <div class=\"container\">\n                            <!-- <h4>{{cabinet._id}}</h4> -->\n                            <p>{{cabinet.provinceLoc}}</p>\n                            <p>{{cabinet.districtLoc}}</p>\n                            <p>{{cabinet.sectorLoc}}</p>     \n                        </div>                    \n                    </div>\n                    <div class=\"nav-bar\">\n                        <ul>\n                            <li><router-link :to=\"{name: 'attorney-dashboard'}\"> @general</router-link></li>\n                            <router-link :to=\"{name: 'attorney-cabinet'}\">@cabinet</router-link>\n                            <li><router-link :to=\"{name: 'attorney-client'}\">@client</router-link></li>\n                            <li><router-link :to=\"{name: 'attorney-services'}\">@services</router-link></li>\n                            <li><a href=\"\">@laws</a></li>\n                        </ul>\n                    </div>\n                </div>\n                <div class=\"action-pane center-pane\">\n                    <p>All your cleint is here </p>\n                    <li v-for=\"(clients, index) in cabinetClient\" :key=\"index\"> \n                       <ul v-for=\"(client, index) in clients.clientDetails\" :key=\"index\">\n                           <li> <a @click=\"getChatWithAttorney(client._id,attorney._id)\" href=\"#\">{{client.fullName}}</a></li>\n                       </ul>\n                    </li>\n                </div>\n                <div class=\"right-pane\">\n                      <div class=\"action-pane searchBox\"></div>                      \n                      <div class=\"action-pane chat-body-area\">\n                          <div v-if=\"startedChat\">\n                               <div v-if=\"!startOfChat\">\n                                    <ul v-for=\"(msg, index) in getMyChat\" :key=\"index\"> \n                                        <li class=\"text-success\">{{msg.message.messageContent}}</li>\n                                        <ul v-for=\"(sender, index) in msg.sendersDetails\" :key=\"index\"> \n                                            <li class=\"bg-info\">{{sender.fullName}}</li>\n                                        </ul>\n                                </ul>\n                               </div>\n                               <div v-else>\n                                     <h3 class=\"text-success\">Start Of Conversation</h3>\n                                     <p class=\"text-warning\">Chat in order to activate the message </p>\n                               </div>\n                           </div>\n                           <div v-else>\n                                    you should choose the one to chat with\n                           </div>\n                      </div>\n                      <div class=\"action-pane chat-input-area\">\n                            <textarea v-model=\"msg\" cols=\"100\" rows=\"2\" placeholder=\"send message generally\"></textarea>\n                            <button @click=\"sendInboxChat(attorney._id)\">SEND</button>\n                      </div>\n                </div>\n              </div>\n              <router-view @refreshData=\"refreshList\"></router-view>\n              \n              \n         </li> \n    </div>     \n</template>\n<script>\n    import http from \"../../http-common\"\n    export default ({\n        data(){\n            return {\n                cabinet: [],\n                generalMsg: [],\n                cabinetClient: [],\n                getMyChat : [], \n                startOfChat: false,  \n                getAttorneyChat: [],  \n                chatingWith: \"\",           \n                startedChat: false,\n                msg: \"\"\n            }\n        },\n        beforeCreate: function () {\n            if (!this.$session.exists()) {\n                    this.$router.push('/loginAttorney')\n            }\n        },\n        methods: {\n            getCabinetCredentials: function(){\n                http.get(\"cabinets/\"+this.$session.get(\"cabinetId\"))\n                    .then(response => {\n                        this.cabinet = response.data.data\n                    })\n            },\n            getCabinetInChat: function(){\n                http.get(\"workspaces/workspaceByCabinetId/\"+this.$session.get(\"cabinetId\"))\n                    .then( response => {\n                        this.cabinetClient = response.data.data\n                    })\n\n            },\n            getChatWithAttorney: function(attorneyId,myId){\n                this.chatingWith = attorneyId\n                this.startedChat = true  \n                // message of the client\n                http.get(\"messages/getChat/\"+attorneyId+\"/\"+myId)\n                    .then(response => {\n                        if(response.data.message){\n                            this.getMyChat = \"\"\n                            this.startOfChat = true\n                        }\n                        else{\n                            this.getMyChat = response.data.data\n                            this.startOfChat = false\n                        }\n                    })\n            },\n            sendInboxChat: function(myId){\n                if(this.msg != \"\"){\n                        let data = {\n                            general :false,\n                            sendersId: myId,\n                            receiversId: this.chatingWith,\n                            messageContent: this.msg\n                        }\n\n                        http.post(\"messages\",data)\n                        this.msg = \"\"\n\n                        this.getChatWithAttorney(this.chatingWith,myId)\n                }\n            },    \n            refreshList: function(){\n                this.getCabinetCredentials()\n                this.getCabinetInChat()\n            }          \n        },\n        mounted() {\n            this.refreshList()\n        }\n    })\n</script>\n"],"sourceRoot":"src/components/attorneyDashboard"}]}